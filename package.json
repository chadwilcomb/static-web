{
  "name": "static-web",
  "version": "1.0.0",
  "description": "static web site template",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "prebuild": "rm -rf public && mkdir public",
    "build": "NODE_ENV=production webpack",
    "surge": "surge -p public -d piq-platform.surge.sh",
    "deploy": "npm run build && npm run surge"
