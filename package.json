    "autoprefixer-core": "^6.0.1",
    "autoprefixer-stylus": "^0.5.0",
    "babel": "^5.1.13",
    "babel-core": "^5.1.13",
    "babel-loader": "^5.0.0",
    "bootstrap-styl": "^4.0.5",
    "css-loader": "^0.12.0",
    "express": "^4.13.0",
    "file-loader": "^0.8.1",
    "font-awesome": "^4.3.0",
    "hjs-webpack": "^2.13.0",
    "local-links": "^1.4.0",
    "postcss-loader": "^0.6.0",
    "react": "^0.13.2",
